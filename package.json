{
  "name": "atm-controller",
  "version": "1.0.0",
  "description": "Simple APM controller",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "compile": "tsc build",
    "test": "jest",
    "lint": "eslint ./src --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "diagnostics": true
        }
      ]
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "8.22.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  }
}
